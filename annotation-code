def trigger_1():
    while True:
        a = input("what violation is in primary cluster?\n")
        if a == "dff":
            onedff = input("link your screenshot here: \n")
            break
        elif a == "web":
            oneweb = input("link your screenshot here: \n")
        elif a == "atd":
            oneatd = input("link your screenshot here: \n")
        elif a == "gmb":
            onegmb = input("link your screenshot here: \n")
        elif a == "sex":
            onesex = input("link your screenshot here: \n")
        elif a == "vio":
            onevio = input("link your screenshot here: \n")
        elif a == "iarc":
            oneiarc = input("link your screenshot here: \n")
        elif a == "geo":
            onegeo = input("link your screenshot here: \n")
        elif a == "inc":
            oneinc = input("link your screenshot here: \n")
        elif a == "na":
            onena = "no violation in first cluster."
            break
        anything = input("anything else?\n")
        if anything == "Y":
            continue
        else:
            break   
    trigger_2()
    print(a)





